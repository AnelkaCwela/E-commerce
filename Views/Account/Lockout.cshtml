@{
    ViewData["Title"] = "Account Locked";
}
<h4 class="text-center alert alert-danger alert-heading">Your Account has been locked!</h4>
<hr />
<div class="alert alert-danger alert-dismissible fade show">
    <strong>Your Account has been locked!</strong>  Please  try again after 5 min.
    <hr />
    Rest your password by clicking <a asp-controller="Account" asp-action="ForgortPassword">Reset</a>
</div>
